<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<StackPane prefWidth="231.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="application.MainController">
  <children>
    <StackPane>
      <children>
        <BorderPane prefHeight="200.0" prefWidth="200.0">
          <bottom>
            <HBox prefHeight="100.0" prefWidth="200.0">
              <children>
                <Label text="Session ID:" />
                <Label fx:id="sessionLabel" text="Undefined" />
                <GridPane prefHeight="99.99990000000253" prefWidth="389.0">
                  <children>
                    <Button fx:id="cameraButton" mnemonicParsing="false" onAction="#startCamera" text="Start Camera" GridPane.columnIndex="0" GridPane.halignment="CENTER" GridPane.rowIndex="0" GridPane.valignment="CENTER" />
                    <Button mnemonicParsing="false" text="Capture Image" GridPane.columnIndex="0" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER" />
                    <Button id="loginButton" fx:id="logoutButton" alignment="CENTER" defaultButton="true" mnemonicParsing="false" text="Logout" textAlignment="CENTER" GridPane.columnIndex="0" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER" />
                  </children>
                  <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="148.0" minWidth="10.0" prefWidth="109.0" />
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="228.0" minWidth="10.0" prefWidth="189.0" />
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="228.0" minWidth="10.0" prefWidth="189.0" />
                  </columnConstraints>
                  <rowConstraints>
                    <RowConstraints maxHeight="34.0" minHeight="10.0" prefHeight="34.0" vgrow="SOMETIMES" />
                    <RowConstraints maxHeight="33.0" minHeight="10.0" prefHeight="33.0" vgrow="SOMETIMES" />
                    <RowConstraints maxHeight="33.0" minHeight="10.0" prefHeight="33.0" vgrow="SOMETIMES" />
                  </rowConstraints>
                  <HBox.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                  </HBox.margin>
                </GridPane>
              </children>
            </HBox>
          </bottom>
          <left>
            <VBox prefHeight="200.0" prefWidth="100.0">
              <children>
                <Label text="Camera Feed" />
                <ImageView id="cameraFeed" fx:id="originalImage" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                <TextArea prefWidth="200.0" text="Face recognition statistics to be displayed here" wrapText="true" />
              </children>
            </VBox>
          </left>
          <right>
            <VBox prefHeight="200.0" prefWidth="100.0">
              <children>
                <VBox prefHeight="200.0" prefWidth="100.0">
                  <children>
                    <Label text="Captured Image" />
                    <ImageView fx:id="capturedImage" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                  </children>
                </VBox>
                <VBox prefHeight="200.0" prefWidth="100.0">
                  <children>
                    <Label text="Greyscale+ Histogram average" />
                    <ImageView fx:id="greyscaleImage" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                  </children>
                </VBox>
                <VBox prefHeight="200.0" prefWidth="100.0">
                  <children>
                    <Label text="Eigen Face Detected" />
                    <ImageView fx:id="eigenFaces" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                  </children>
                </VBox>
              </children>
            </VBox>
          </right>
        </BorderPane>
      </children>
    </StackPane>
  </children>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
</StackPane>
